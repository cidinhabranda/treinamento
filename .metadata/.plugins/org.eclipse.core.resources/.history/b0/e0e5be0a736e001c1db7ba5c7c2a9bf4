package entities;

public class Student {
	public String nome;
	public double notaA;
	public double notaB;
	public double notaC;
	
	
	
	public double somaNota() {
		return notaA + notaB + notaC;
		
	}
	public String verficaAluno() {
		if (somaNota() >=60) {
			System.out.println("FINAL GRADE = "  + this.somaNota());
			return "Pass";
			
		}
		return "Failed";
		
	}

	
}

	
	
	
	
